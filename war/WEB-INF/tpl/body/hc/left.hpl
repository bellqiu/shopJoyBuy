<script>
	jq("#helpcenter").ready(function(){
		imgSite = "${siteView.imageHost!"http://www.joybuy.co.uk"}";
		jq('img[id^="collapse"]').click(function(){
			var ulId = this.id+"_content";
			if(this.src == imgSite + "/style/image/pic/sanjiao.png"){
				this.src = imgSite + "/style/image/pic/sanjiao-x.png";
				jq('ul[id='+ulId+']').show();
			} else {
				this.src = imgSite + "/style/image/pic/sanjiao.png";
				jq('ul[id='+ulId+']').hide();
			}
		});
	});
</script>
