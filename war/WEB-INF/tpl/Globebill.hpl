<#assign pageKeywords = "Globebill, Joybuy.com"> 
<#assign pageDescription = "Pay by Globebill"> 
<#assign pageTitle = "Pay by Globebill">
<#include "/header/top_clean.hpl">
<p><img style="margin-left: 300px;margin-top: 160px" alt="Loading" src="/css/loading_img_s.gif"></p>
<div>
	<#assign defaultOrder = currentOrder>
	<!-- 
			merNo + gatewayNo +
			orderNo + orderCurrency+ orderAmount +
			returnUrl+ signkey
	 -->
	 
	<form name="payForm"
		action="https://payment.secure-checkoutserver.com/Interface"
		method="post" id="paypaysubmitForm">
		<input type="hidden" name="merNo" value="10309" />
		 <input type="hidden" name="gatewayNo" value="10309001" /> 
		 <input type="hidden" name="orderNo" value="${defaultOrder.name}" />
		 <input type="hidden" name="orderCurrency" value="${defaultOrder.currency}" />
		 <input type="hidden" name="orderAmount" value="${amount}" /> 
		 <input type="hidden" name="signInfo" value="${signInfo}" />
		<input type="hidden" name="returnUrl" value="${returnUrl}" /> 
		<input type="hidden" name="firstName" value="${firstName}" /> 
		<input type="hidden" name="lastName" value="${lastName!'N/A'}" />
		<input type="hidden" name="email" value="${defaultOrder.customerEmail}" /> 
		<input type="hidden" name="phone" value="${defaultOrder.customerTelephone}" /> 
		<input type="hidden" name="paymentMethod" value="Credit Card" /> 
		<input type="hidden" name="country" value="${cc}" /> 
		<input type="hidden" name="city" value="${defaultOrder.city}" />
		<input type="hidden" name="address" value="${defaultOrder.customerAddress}" />
		<input type="hidden" name="zip" value="${defaultOrder.customerZipcode}" /> 
	</form>
	<script type="text/javascript">
		jq("#paypaysubmitForm").submit();
	</script>
</div>

