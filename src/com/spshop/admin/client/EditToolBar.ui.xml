<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		.container{
			background:#FFF;
		}
		
	</ui:style>
	<g:HorizontalPanel height="2EM" styleName="{style.container}" >
		<g:HorizontalPanel>
			<g:Button styleName="{style.important}" ui:field="save" >Save</g:Button>
			<g:Button styleName="{style.important}" ui:field="delete" >Delete</g:Button>
		</g:HorizontalPanel>
	</g:HorizontalPanel>
</ui:UiBinder> 